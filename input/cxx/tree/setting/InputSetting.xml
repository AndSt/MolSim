<?xml version="1.0"?>
<pse
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="config.xsd">

	<start_time>0.0</start_time>
	<t_end>5</t_end>
	<delta_t>0.0002</delta_t>
	
	<!-- Lennard-Jones Force --> 
	<ljf>
		<!-- potential well --> 
		<epsilon>5.0</epsilon>
		<!-- zero crossing --> 
		<sigma>1.0</sigma>
	</ljf>
	
	<inputfile>
		<name>InputCuboids.xml</name>
		<!-- particles/cuboids/spheres -->
		<type>cuboids</type>
	</inputfile>
	
	<outputfile>
		<name>MolSimVTK</name>
		<freq>10</freq>
	</outputfile>
</pse>
